{
    "type": "rich",
    "title": "${settings.name.value}",
    "description": "${settings.motd.value}",
    "color": 4298035,
    "fields": [
        {
            "name": "Players",
            "value": "${regex.players} of ${regex.max-players}",
            "inline": true
        },
        {
            "name": "Version",
            "value": "${settings.game-version}",
            "inline": true
        },
        {
            "name": "Host",
            "value": "${server.node.publicHost}",
            "inline": true
        }
    ],
    "author": {
        "name": "${server.name}",
        "url": "${self.server.url}",
        "icon_url": "https://cdn.discordapp.com/attachments/929980406135132191/1020712827142537296/unknown.png"
    }
}