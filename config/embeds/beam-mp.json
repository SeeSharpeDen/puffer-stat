{
    "type": "rich",
    "title": "${settings.name.value}",
    "description": "${settings.description.value}",
    "color": 65535,
    "fields": [
        {
            "name": "Players",
            "value": "${regex.0}",
            "inline": true
        },
        {
            "name": "Uptime",
            "value": "${regex.1}ms",
            "inline": true
        },
        {
            "name": "Host",
            "value": "${server.node.publicHost}",
            "inline": true
        }
    ],
    "author": {
        "name": "${server.name}",
        "url": "${self.server.url}",
        "icon_url": "https://beammp.com/img/mainlogo.png"
    }
}